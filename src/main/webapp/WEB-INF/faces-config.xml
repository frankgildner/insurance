<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.0"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
    <navigation-rule>
        <from-view-id>/login.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/profile.xhtml</to-view-id>
            <from-outcome>settings</from-outcome>
            <to-view-id>/settings.xhtml</to-view-id>
            <from-outcome>policies</from-outcome>
            <to-view-id>/policies.xhtml</to-view-id>
            <from-outcome>damagecases</from-outcome>
            <to-view-id>/damagecases.xhtml</to-view-id>
            <from-outcome>register</from-outcome>
            <to-view-id>/register.xhtml</to-view-id>
            <from-outcome>administration</from-outcome>
            <to-view-id>/administration.xhtml</to-view-id>
            <from-outcome>policydetail</from-outcome>
            <to-view-id>/policydetail.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>